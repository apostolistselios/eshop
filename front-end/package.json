    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@primeuix/themes": "^2.0.2",
    "bootstrap": "^5.2.3",
    "primeicons": "^7.0.0",
    "primeng": "^21.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "jsdom": "^27.1.0",
    "prettier": "^3.7.4",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
